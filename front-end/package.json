{
	"name": "kitchen",
	"version": "0.1.0",
	"private": true,
	"main": "electron/main.js",
	"homepage": "./",
	"build": {
		"appId": "com.sooonism.pipidance",
		"mac": {
			"target": "dmg",
			"icon": "icon/icon-cupcake.icns"
		}
	},
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"axios": "^0.19.2",
		"classnames": "^2.2.6",
		"electron-installer-dmg": "^3.0.0",
		"electron-is-dev": "^1.1.0",
		"electron-packager": "^14.2.0",
		"firebase": "^7.10.0",
		"firestore": "^1.1.6",
		"menubar": "^7.2.0",
		"node-sass-chokidar": "^1.4.0",
		"npm-run-all": "^4.1.5",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-scripts": "3.3.0"
	},
	"scripts": {
		"start-js": "react-scripts start",
		"start": "npm-run-all -p watch-css start-js",
		"build": "npm run build-css && react-scripts build",
		"build-css": "node-sass-chokidar src/sass -o src/css",
		"watch-css": "npm run build-css && node-sass-chokidar src/sass -o src/css --watch --recursive",
		"eject": "react-scripts eject",
		"electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
		"package": "electron-builder build --mac --publish never"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"concurrently": "^5.1.0",
		"electron": "^9.4.0",
		"electron-builder": "^22.3.2",
		"wait-on": "^4.0.0"
	}
}
